{
	"name": "mf",
	"type": "module",
	"version": "0.0.1",
	"scripts": {
		"dev": "astro dev",
		"build": "astro build",
		"preview": "astro preview",
		"astro": "astro",
		"tsc": "tsc",
		"tsc:check": "tsc --noEmit",
		"optimize:images": "bun scripts/optimize-images.mjs",
		"optimize:videos": "bun scripts/optimize-videos.mjs",
		"format": "bunx prettier . --write",
		"lint": "bun x eslint .",
		"lint:fix": "bun x eslint . --fix",
		"prepare": "husky install",
		"deploy:worker": "astro build && bunx wrangler deploy",
		"pre-commit": "bun run tsc:check && bun run lint && bun run format"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.7",
		"astro": "5.8.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"lucide-static": "^0.511.0",
		"tailwind-merge": "^3.3.0",
		"tailwindcss": "^4.1.7"
	},
	"devDependencies": {
		"@eslint/js": "^9.27.0",
		"@typescript-eslint/parser": "8.33.0",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-astro": "^1.3.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.4.1",
		"globals": "^16.2.0",
		"husky": "^8.0.0",
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"sharp": "^0.34.2",
		"tw-animate-css": "^1.3.0",
		"typescript": "5.8.3",
		"typescript-eslint": "^8.33.0",
		"wrangler": "^4.16.1"
	}
}
